	"BuildDefinedId": "#{BuildDefinedId}#",
	"SourceConfigUrl": "https://raw.githubusercontent.com/lean-soft/labs-templates/master/000-image-sync/labs-vm-snapshot-global-config.json",
	"SourceSnapshot": {
		"UserGlobalOrSelf":"global",
		"ResourceGroupName": "#{resourceGroupName}#",
