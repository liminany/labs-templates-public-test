{
	"Services": [
		{
			"uniqueName":"LabsVMImageCopy",
			"displayname": "LabsVMImageCopy",					
			"properties":[				
				{
					"key":"sourceAzureAccountName",
					"value":"#{sourceAzureAccountName}#",
					"type":"text"
				},
				{
					"key":"sourceSubscriptionId",
					"value":"#{sourceSubscriptionId}#",
					"type":"text"
				},
				{
					"key":"sourceResourceGroupName",
					"value":"#{resourceGroupName}#",
					"type":"text"
				},	
				{
					"key":"sourceSnapshotName",
