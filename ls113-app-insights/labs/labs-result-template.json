{
	"Services": [
		{
			"uniqueName":"AppInsights",
			"displayname": "Application Insights",
			"properties":[
				{
					"key":"Application Name",
					"value":"#appName"
				},
				{
					"key":"Instrumentation Key",
					"value":"#instrumentationKey"
				},
				{
					"key":"Portal URL",
