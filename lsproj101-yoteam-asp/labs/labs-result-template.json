{
	"Services": [
		{
			"uniqueName":"YoTeamAsp",
			"displayname": "Generate Asp.Net Core Project CI/CD Demo",
			"hostname": "#{tfs}#",			
			"properties":[				
				{
					"key":"type",
					"value":"#{type}#",
					"type":"text"
				},
				{
					"key":"Application Name",
					"value":"#{applicationName}#",
					"type":"text"
				},
				{
					"key":"Project Home",
					"value":"#{tfs}#/#{applicationName}#/_home",
					"type":"url"
				},
				{
					"key":"Git Rep URL",
					"value":"#{tfs}#/_git/#{applicationName}#",
					"type":"url"
				},
				{
					"key":"Build Pipelines",
					"value":"#{tfs}#/#{applicationName}#/_build",
					"type":"url"
				},
				{
					"key":"Release Pipelines",
					"value":"#{tfs}#/#{applicationName}#/_release",
					"type":"url"
				},
				{
					"key":"Application endpoint",
					"value":"http://#{applicationName}#.azurewebsites.net",
					"type":"url"
				},
				
			]
		}
	],
	"Info": {
		"Subscription": {
			"Id": "#{azureSubId}#",
		},
		"ResourceGroup": {
			"Name": "#{applicationName}#",
			"Location": ""
		}
	}
}