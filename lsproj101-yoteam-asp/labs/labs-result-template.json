{
	"Services": [
		{
			"uniqueName":"YoTeamAsp",
			"displayname": "Generate Asp.Net Core Project CI/CD Demo",					
			"properties":[				
				{
					"key":"type",
					"value":"#{type}#",
					"type":"text"
				},
				{
					"key":"Application Name",
					"value":"#{applicationName}##{Build.BuildId}#",
					"type":"text"
				},
				{
					"key":"Project Home",
					"value":"#{tfs}#/#{applicationName}##{Build.BuildId}#/_home",
					"type":"url"
				},
				{
					"key":"Git Rep URL",
					"value":"#{tfs}#/_git/#{applicationName}##{Build.BuildId}#",
					"type":"url"
				},
				{
					"key":"Build Pipelines",
					"value":"#{tfs}#/#{applicationName}##{Build.BuildId}#/_build",
					"type":"url"
				},
				{
					"key":"Release Pipelines",
					"value":"#{tfs}#/#{applicationName}##{Build.BuildId}#/_release",
					"type":"url"
				},
				{
					"key":"Application endpoint",
					"value":"http://#{applicationName}##{Build.BuildId}#.azurewebsites.net",
					"type":"url"
				},
				
			]
		}
	],
	"Info": {
		"Subscription": {
			"Id": "#{azureSubId}#",
		},
		"ResourceGroup": {
			"Name": "#{applicationName}##{Build.BuildId}#",
			"Location": ""
		}
	}
}